{
  "name": "@zidendev/zidenjs",
  "type": "module",
  "version": "1.0.1",
  "description": "Core library for interacting with Ziden protocol, supports all functions for holders, issuers and verifiers",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepack": "npmignore --auto .npmignore",
    "test-all": "ts-mocha -p tsconfig.json src/**/*.test.ts",
    "test": "ts-mocha -p tsconfig.json",
    "prepare": "husky install && npm run build",
    "format": "prettier '*/**/*.{ts,js,json,yml,md}' --write",
    "build": "tsc --project tsconfig.build.json"
  },
  "publishConfig": {
    "access": "public",
    "ignore": [
      "src",
      ".eslintrc.cjs",
      ".husky",
      ".mocharc.cjs",
      ".prettierrc",
      ".vscode/settings.json",
      "tsconfig.build.json",
      "tsconfig.json"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "big-integer": "^1.6.51",
    "bigint-buffer": "^1.1.5",
    "blake-hash": "^2.0.0",
    "circomlib": "^2.0.5",
    "level": "^8.0.0",
    "wasmbuilder": "^0.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "chai": "^4.3.6",
    "circom_tester": "^0.0.18",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "npmignore": "^0.3.0",
    "prettier": "^2.7.1",
    "snarkjs": "^0.5.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
